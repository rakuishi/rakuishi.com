{
  "hosting": {
    "public": "out",
    "headers": [
      {
        "source": "**/*.@(jpg|gif|png|svg|webp|woff2)",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "max-age=604800"
          }
        ]
      }
    ],
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ]
  }
}